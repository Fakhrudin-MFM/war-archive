<%
const staticPrefix = `${module}/vov/`;
%>
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <base id="baseUrl" href="<%= baseUrl %>">

  <title>Портал поиска документов</title>
  <link rel="icon" href="<%= staticPrefix %>images/favicon.png">
  <link href="<%= staticPrefix %>css/bootstrap.min.css" rel="stylesheet">

  <link href="<%= staticPrefix %>css/fonts.css" rel="stylesheet">
  <link href="<%= staticPrefix %>css/style.css" rel="stylesheet">
  <link href="<%= staticPrefix %>css/animate.css" rel="stylesheet">
  <link href="<%= staticPrefix %>css/magnific-popup.css" rel="stylesheet">
  <link href="<%= staticPrefix %>css/linear-icons.css" rel="stylesheet">

  <link href="https://fonts.googleapis.com/css?family=Raleway:400,700,800,900" rel="stylesheet">
  <link href="<%= staticPrefix %>css/font-awesome.min.css" rel="stylesheet">
  <link href="<%= module %>/vendor/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">
  <link href="<%= staticPrefix %>css/custom.css" rel="stylesheet">

  <%- stylesheets %>

  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>

<body class="one-page" data-spy="scroll" data-target="#nav-scroll">

<div id="preloader">
  <div class="spinner spinner-round"></div>
</div>

<div id="top">
</div>

<%- body -%>

<%- partial("../parts/footer") %>

<script src="<%= staticPrefix %>js/mp/jquery.min.js"></script>
<script src="<%= staticPrefix %>js/mp/bootstrap.min.js"></script>
<script src="<%= staticPrefix %>js/mp/jquery.easing.min.js"></script>
<script src="<%= staticPrefix %>js/mp/custom.js"></script>
<script src="<%= staticPrefix %>js/mp/jquery.shuffle.min.js"></script>
<script src="<%= staticPrefix %>js/mp/jquery.magnific-popup.min.js"></script>
<script>
  // This will create a single gallery from all elements that have class "lightbox"
  $('.lightbox').each(function () {
    $(this).magnificPopup({
      delegate: 'a.open-gallery', // the selector for gallery item
      type: 'image',
      gallery: {
        enabled: true
      }
    });
  });
</script>
<script src="<%= staticPrefix %>js/mp/validator.min.js"></script>
<script src="<%= staticPrefix %>js/mp/preloader.js"></script>
<script src="<%= staticPrefix %>js/mp/smooth-scroll.js"></script>
<script src="<%= staticPrefix %>js/mp/jquery.dataTables.min.js"></script>
<script src="<%= module %>/vendor/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>

<%- scripts %>

<%- partial("../parts/yandex-metrika") %>

</body>

</html>